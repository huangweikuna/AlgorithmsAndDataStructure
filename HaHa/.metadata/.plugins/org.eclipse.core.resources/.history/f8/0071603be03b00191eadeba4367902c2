package recursion;

/*
 * 226. Invert Binary Tree
 * 		·­×ªÁ½¿Ã¶þ²æÊ÷
 */
public class LeetCode226 {
	public TreeNode invertTree(TreeNode root) {
		if(root == null)
			return null;
		invertTree(root.left);
		invertTree(root.right);
		TreeNode node = new TreeNode(0);
		node = root.right;
		root.right = root.left;
		root.left = node;
	}
}